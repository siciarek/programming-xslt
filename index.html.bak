<script type="text/javascript">
	function show_fractal(name, direct) {
		
		direct = direct || false;
		
		if(direct == true)
		{
			var console = document.getElementById('console');
			console.src = name;
		}
		else
		{
			show_in_console(name, 'fractals');
		}
	}

	function show_function(name) {
		show_in_console(name, 'recursion');
	}

	function show_animation(name) {
		var console = document.getElementById('console');
		var img = '/images/' + name + '/animation.gif';
		console.src = img;
	}

	function show_in_console(name, directory) {
		var console = document.getElementById('console');
		console.src = '/wait.html';
		setTimeout(function(){console.src = '/' + directory + '/' + name + '.xml';}, 500);
		
	}
</script>

